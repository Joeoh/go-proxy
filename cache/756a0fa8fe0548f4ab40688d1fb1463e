		$(function() {
			// Core Tabs behaviour.
			// Adds a more button, which shows and hides items with the class more.
			var core_tabs = $('div.tcd-tabs');
			var more_link = $('a.tcd-tabs-more', core_tabs);
			more_menu = $('ul.tcd-tabs-more-menu').css({opacity:0}).remove();
